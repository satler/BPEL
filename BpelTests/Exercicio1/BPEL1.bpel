<process name="BPEL1"
               targetNamespace="http://xmlns.oracle.com/BpelTests/Exercicio1/BPEL1"
               xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
               xmlns:client="http://xmlns.oracle.com/BpelTests/Exercicio1/BPEL1"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:task="http://xmlns.oracle.com/bpel/workflow/task"
         xmlns:taskservice="http://xmlns.oracle.com/bpel/workflow/taskService"
         xmlns:wfcommon="http://xmlns.oracle.com/bpel/workflow/common"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/db/BpelTests/Exercicio1/tenteBanco"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/BpelTests/Exercicio1/testDB">

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="bpel1_client" partnerLinkType="client:BPEL1" myRole="BPEL1Provider"/>
    <partnerLink name="testDB" partnerLinkType="ns2:testDB_plt"
                 partnerRole="testDB_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:BPEL1RequestMessage"/>

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="client:BPEL1ResponseMessage"/>
    <variable name="Invoke1_tenteBanco_InputVariable"
              messageType="ns1:tenteBancoInput_msg"/>
    <variable name="Invoke1_tenteBanco_OutputVariable"
              messageType="ns1:tenteBancoOutputCollection_msg"/>
    <variable name="Invoke1_testDBSelect_InputVariable"
              messageType="ns2:testDBSelect_inputParameters"/>
    <variable name="Invoke1_testDBSelect_OutputVariable"
              messageType="ns2:TestemsgCollection_msg"/>
    <variable name="Invoke1_testDBSelect_InputVariable_1"
              messageType="ns2:testDBSelect_inputParameters"/>
    <variable name="Invoke1_testDBSelect_OutputVariable_1"
              messageType="ns2:TestemsgCollection_msg"/>
  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in BPEL1.wsdl) -->
    <receive name="receiveInput" partnerLink="bpel1_client" portType="client:BPEL1" operation="process" variable="inputVariable" createInstance="yes"/>
    <invoke name="Invoke1" bpelx:invokeAsDetail="no" bpelx:conversationId="8"
            inputVariable="Invoke1_testDBSelect_InputVariable_1"
            outputVariable="Invoke1_testDBSelect_OutputVariable_1"
            partnerLink="testDB" portType="ns2:testDB_ptt"
            operation="testDBSelect"/>
    <assign name="Transform1">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:doXSLTransformForDoc('xsl/Transformation_2.xsl', $Invoke1_testDBSelect_OutputVariable_1.TestemsgCollection)"/>
        <to variable="Invoke1_testDBSelect_OutputVariable_1"
            part="TestemsgCollection"/>
      </copy>
    </assign>
    <assign name="teste">
      <copy>
        <from expression="concat('Funciona? ',bpws:getVariableData('inputVariable','payload','/client:process/client:input'))"/>
        <to variable="outputVariable" part="payload"
            query="/client:processResponse/client:result"/>
      </copy>
    </assign>
    <!-- Generate reply to synchronous request -->
    <reply name="replyOutput" partnerLink="bpel1_client" portType="client:BPEL1" operation="process" variable="outputVariable"/>
  </sequence>
</process>